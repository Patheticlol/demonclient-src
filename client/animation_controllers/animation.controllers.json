{
	"format_version":"1.10.0",
	"animation_controllers":{
		"controller.animation.fps":{
			"initial_state":"default",
			"states":{
				"default":
				{
					"on_entry":["variable.life_time = query.life_time;"],
					"transitions":[{"theBrain":"(query.life_time - variable.life_time)>= 1.5"}]
				},
				"theBrain":
				{
					"on_entry":["variable.fps = (0.1/query.delta_time);"],
					"transitions":[{"default":"1.0"}]
				}
			}
		}
	}
}